{
  "output": {
    "filePath": "repomix-output.xml",
    "style": "xml"
  },
  "include": [
    "src/**/*",
    "prisma/schema.prisma",
    "package.json",
    "tsconfig.json",
    "next.config.js",
    "jest.config.ts"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "*.backup",
      "*.backup-*",
      "prisma/migrations/**",
      "repomix-output.*",
      "node_modules/**",
      "storage/**",
      "logs/**",
      ".git/**"
    ]
  }
}